<h1>CampoExtendido list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Descripcion</th>
            <th>Requerido</th>
            <th>Orden</th>
            <th>Estilocss</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('campoextendido_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.descripcion }}</td>
            <td>{{ entity.requerido }}</td>
            <td>{{ entity.orden }}</td>
            <td>{{ entity.estiloCss }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('campoextendido_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('campoextendido_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('campoextendido_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
