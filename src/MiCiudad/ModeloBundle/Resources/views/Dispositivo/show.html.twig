<h1>Dispositivo</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Dispositivo</th>
            <td>{{ entity.dispositivo }}</td>
        </tr>
        <tr>
            <th>Version</th>
            <td>{{ entity.version }}</td>
        </tr>
        <tr>
            <th>Detalle</th>
            <td>{{ entity.detalle }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('dispositivo') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('dispositivo_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('dispositivo_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
